    "build": "yarn clean && yarn build:modfed && yarn build:node",
    "build-storybook": "build-storybook",
    "build:babel": "babel --extensions \".js,.jsx,.ts,.tsx\" ./src --out-dir ./dist --ignore \"**/*.stories.tsx,**/*.test.ts,**/*.test.tsx,**/*.d.ts\"",
    "build:cjs": "rm -rf dist && cross-env NODE_ENV=production BABEL_ENV=cjs yarn build:babel && yarn build:typescript && babel-node scripts/mapTypeDefinitionFiles.js",
    "build:mf:stats": "yarn clean && cross-env ANALYZER=true yarn build:modfed",
    "build:modfed": "cross-env NODE_ENV=production webpack --config webpack.config.modfed.js --progress",
    "build:modfed:watch": "cross-env NODE_ENV=development webpack --config webpack.config.modfed.js --watch --progress",
    "build:node": "cross-env NODE_ENV=production webpack --config webpack.config.node.js --progress",
    "build:node:watch": "cross-env NODE_ENV=development webpack --config webpack.config.node.js --watch --progress",
    "build:pkg": "rm -rf dist && yarn build:cjs",
    "build:stats": "yarn clean && cross-env ANALYZER=true yarn build:node",
    "build:typescript": "rm -rf build-ts && tsc --project tsconfig.json",
    "build:watch": "rm -rf dist && concurrently -r --kill-others \"yarn build:modfed:watch\" \"yarn build:node:watch\"",
    "clean": "rm -rf build dist",
    "coverage": "rm -rf coverage && yarn test --json --outputFile=.jest-test-results.json --coverage",
    "federated:server": "http-server ./build -p 3010",
    "generate:comp": "babel-node scripts/generateComponent.js --name",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx",
    "mock": "babel-node server/server.js",
    "publish:local": "IS_LOCAL_ONLY=true yarn publish:npm",
    "publish:npm": "babel-node scripts/publish-npm.js",
    "publish:pkg": "babel-node scripts/webpack/publish.js ",
    "release": "yarn release:patch",
    "release:alpha": "CI=true RELEASE_TYPE=alpha yarn publish:npm",
    "release:major": "PUSH_VERSION=true RELEASE_TYPE=major yarn publish:npm",
    "release:minor": "PUSH_VERSION=true RELEASE_TYPE=minor yarn publish:npm",
    "release:patch": "PUSH_VERSION=true RELEASE_TYPE=patch yarn publish:npm",
    "setup": "yarn && yarn coverage",
    "story": "start-storybook -s ./public -p 3000 -c .storybook --ci",
    "storybook": "yarn clean && concurrently -r --kill-others \"yarn build:modfed:watch\" \"yarn federated:server\" \"yarn story\"",
